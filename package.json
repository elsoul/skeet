{
  "name": "@skeet-framework/cli",
  "version": "1.13.0",
  "description": "Skeet CLI",
  "main": "dist/index.js",
  "repository": "https://github.com/elsoul/skeet-cli",
  "keywords": [
    "openai",
    "firestore",
    "firebase",
    "GCP",
    "TypeScript",
    "GraphQL",
    "Prisma",
    "framework",
    "AI"
  ],
  "author": "ELSOUL LABO B.V.",
  "license": "Apache-2.0",
  "bin": {
    "skeet": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx --fix .",
    "test": "jest --coverage=false --detectOpenHandles --maxWorkers=1",
    "copy-assets": "copyfiles -u 4 src/cli/ai/locales/**/skeetAi.json dist/locales/",
    "update:packages": "ncu -u --reject node-fetch && yarn",
    "build": "npx ts-node build.ts && npx ts-node add-line.ts && yarn copy-assets && yarn postbuild",
    "postbuild": "copyfiles -u 1 src/lib/locales/*.json dist/",
    "publish": "npm publish",
    "dev": "nodemon",
    "start:dev": "npx ts-node build.ts && node ./dist/index.js",
    "start": "node dist/index.js",
    "skeet:skeet": "yarn --cwd ./functions/skeet dev",
    "discord:labo": "npx ts-node -r tsconfig-paths/register --transpile-only src/lib/runDiscordChangeLog.ts labo",
    "discord:epics": "npx ts-node -r tsconfig-paths/register --transpile-only src/lib/runDiscordChangeLog.ts epics",
    "skeet:yoo": "yarn --cwd ./functions/yoo dev"
  },
  "dependencies": {
    "@skeet-framework/ai": "^1.7.11",
    "@skeet-framework/utils": "^1.2.4",
    "chalk": "5.3.0",
    "chalk-pipe": "6.0.0",
    "cli-spinner": "0.2.10",
    "cli-table3": "0.6.3",
    "commander": "11.1.0",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.3.0",
    "i18n": "0.15.1",
    "inquirer": "9.2.12",
    "jest": "29.7.0",
    "node-fetch": "2.6.9",
    "prompt": "1.3.0",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@skeet-framework/discord-utils": "0.2.14",
    "@skeet-framework/firestore": "^2.4.2",
    "@stdlib/string-percent-encode": "0.1.1",
    "@types/cli-spinner": "0.2.3",
    "@types/i18n": "^0.13.10",
    "@types/inquirer": "9.0.7",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/node-fetch": "2.6.9",
    "@types/prompt": "1.1.8",
    "@types/semver": "7.5.6",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "babel-loader": "9.1.3",
    "copyfiles": "2.4.1",
    "esbuild": "0.19.9",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "nodemon": "3.0.2",
    "npm-check-updates": "16.14.11",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}