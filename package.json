{
  "name": "@skeet-framework/cli",
  "version": "0.0.1",
  "description": "Skeet CLI",
  "main": "dist/index.js",
  "repository": "https://github.com/elsoul/skeet-cli.git",
  "keywords": [
    "openai",
    "firestore",
    "firebase",
    "GCP",
    "TypeScript"
  ],
  "author": "ELSOUL LABO B.V.",
  "private": false,
  "license": "Apache-2.0",
  "bin": {
    "skeet": "./dist/index.js"
  },
  "scripts": {
    "test": "jest --coverage=false --detectOpenHandles --maxWorkers=1",
    "update:packages": "ncu -u && yarn",
    "update:version": "ts-node -r tsconfig-paths/register release.ts",
    "build": "yarn update:version && npx ts-node build.ts && npx ts-node add-line.ts",
    "publish": "npm publish",
    "dev": "nodemon",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "chalk": "5.2.0",
    "chalk-pipe": "6.0.0",
    "commander": "10.0.0",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.1.0",
    "inquirer": "9.1.5",
    "jest": "29.5.0",
    "node-fetch": "3.3.1",
    "prompt": "1.3.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/inquirer": "9.0.3",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.10",
    "@types/prompt": "1.1.5",
    "babel-loader": "9.1.2",
    "esbuild": "0.17.14",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "nodemon": "2.0.22",
    "npm-check-updates": "16.8.0",
    "prettier": "2.8.7",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "5.0.2"
  }
}
