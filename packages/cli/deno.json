{
  "name": "@skeet-framework/cli",
  "version": "3.0.0rc",
  "exports": "./dist/mod.ts",
  "publish": {
    "include": ["src"],
    "exclude": ["src/tests"]
  },
  "tasks": {
    "dev": "deno run -A --watch src/index.ts",
    "publish": "deno publish",
    "start": "deno run -A src/index.ts",
    "install": "deno install -Afg src/index.ts --name saver --import-map=deno.json --allow-scripts=npm:bigint-buffer@1.1.5,npm:bufferutil@4.0.8,npm:utf-8-validate@5.0.10"
  },
  "imports": {
    "@/": "./src/",
    "@/lib": "./src/lib/",
    "@common/": "../../common/",
    "/": "./",
    "@cliffy": "https://deno.land/x/cliffy@v1.0.0-rc.4/command/mod.ts",
    "@std/assert": "jsr:@std/assert@1"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  }
}
