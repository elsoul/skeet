{
  "name": "@skeet-framework/cli",
  "version": "2.0.6",
  "description": "Skeet CLI",
  "main": "dist/index.js",
  "keywords": [
    "openai",
    "firestore",
    "firebase",
    "GCP",
    "TypeScript",
    "Prisma",
    "framework",
    "AI"
  ],
  "author": "ELSOUL LABO B.V.",
  "homepage": "https://labo.elsoul.nl",
  "bugs": {
    "url": "https://discord.gg/ausnBvAM38"
  },
  "repository": "https://github.com/elsoul/skeet.git",
  "license": "Apache-2.0",
  "engines": {
    "pnpm": ">=8",
    "npm": "please_use_pnpm_instead",
    "node": ">=20"
  },
  "bin": {
    "skeet": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx --fix .",
    "test": "vitest",
    "build": "npx ts-node -r tsconfig-paths/register --transpile-only build.ts && npx ts-node -r tsconfig-paths/register --transpile-only add-line.ts && pnpm gen:cli",
    "dev": "npx ts-node build.ts && node ./dist/index.js",
    "start": "node dist/index.js",
    "discord:labo": "npx ts-node -r tsconfig-paths/register --transpile-only src/lib/runDiscordChangeLog.ts labo",
    "discord:epics": "npx ts-node -r tsconfig-paths/register --transpile-only src/lib/runDiscordChangeLog.ts epics",
    "gen:cli": "tsx src/lib/genCLIhelp.ts"
  },
  "dependencies": {
    "@skeet-framework/ai": "workspace:*",
    "@skeet-framework/utils": "workspace:*",
    "chalk": "5.3.0",
    "chalk-pipe": "6.0.0",
    "cli-spinner": "0.2.10",
    "cli-table3": "0.6.3",
    "commander": "12.0.0",
    "dotenv": "16.4.1",
    "dotenv-cli": "7.3.0",
    "i18n": "0.15.1",
    "inquirer": "9.2.14",
    "jest": "29.7.0",
    "prompt": "1.3.0",
    "semver": "7.6.0",
    "vite": "^5.1.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@skeet-framework/discord-utils": "0.3.0",
    "@skeet-framework/firestore": "2.5.0",
    "@stdlib/string-percent-encode": "0.1.1",
    "@types/cli-spinner": "0.2.3",
    "@types/i18n": "0.13.10",
    "@types/inquirer": "9.0.7",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.16",
    "@types/prompt": "1.1.8",
    "@types/semver": "7.5.6",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "babel-loader": "9.1.3",
    "copyfiles": "2.4.1",
    "esbuild": "0.20.0",
    "esbuild-plugin-alias-path": "^2.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "nodemon": "3.0.3",
    "prettier": "3.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.7.1",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.0"
  }
}
