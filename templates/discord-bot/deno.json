{
  "name": "@skeet-framework/discord-bot",
  "exports": {
    "./src/index.ts": "./src/index.ts"
  },
  "tasks": {
    "dev": "deno run -A --watch src/index.ts",
    "serve": "deno run -A src/index.ts",
    "start": "./dist/exe"
  },
  "imports": {
    "@/": "./src/",
    "@/lib": "./src/lib/",
    "@cmn/": "../../cmn/",
    "./": "./",
    "@discord/harmony": "https://deno.land/x/harmony@v2.9.1/mod.ts",
    "@std/assert": "jsr:@std/assert@1"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  }
}
